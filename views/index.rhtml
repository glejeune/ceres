<% @posts.each do |post| %>
	<h1><a href="<%= post.url %>"><%= post.title %></a></h1>
	<div id="feedcontent">
		<%= post.content %>
	</div>
<% end %>

<div id="paginate">
<% unless @previous_page.nil? %>
<a href="<%= URL(Capcode::Index, @previous_page) %>">&laquo; Previous</a>
<% end %>

[<%= @page %>/<%= @number_of_pages %>]

<% unless @next_page.nil? %>
<a href="<%= URL(Capcode::Index, @next_page) %>">Next &raquo;</a>
<% end %>
</div>